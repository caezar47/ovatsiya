{# <!DOCTYPE html> #}
<html dir="ltr" lang="ru-RU"> 
    <head>  
        {% include "../block/common/head/head.twig" with {title:"Контакты"}  %}
    </head>
    <body>
        {% include "../block/common/navbar/navbar.twig" with {
            navbar_prefix:"navbar__", 
            navbar_mod:"is--scroll",
            active:page.contacts,
        } %}

        <main class="{{prefix.section}}wrap">
            {% include "../block/page/contacts.twig" with {
                view:"page", 
                prefix:prefix.section, 
                mod:"is--page-contacts", 
                heading_group:"", 
                heading_mod:"is--h2  is--default", 
                heading_level:"h1", 
                heading:"Контактная информация салона красоты «Овация»",

                fheading_group:"", 
                fheading_mod:"is--h3  is--form-feedback", 
                fheading_level:"h2", 
                fheading:"Напишите <span>нам</span>",

                fbg_left:[path.img,"/default/bg-page.png"].join(''),
                fbg_bottom:[path.img,"/default/bg-form-bottom.png"].join(''),

                agreement_mod:"is--form-agreement", 
                agreement_level:"p", 
                agreement:"* Отправляя данную форму, Вы даете согласие на обработку персональных данных",
            } %} 
        </main>

        {% include "../block/common/footer/footer.twig" with {footer_prefix:"footer__"} %}

        {% include "../block/common/modals/include.twig"%}
        {% include "../block/common/script/script.twig" with {
            aos: 'view'
        }%}
    </body>
</html> 
