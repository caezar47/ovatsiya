{% for value in list %}
	<div class="{{prefix}}card  {{mod}}  {{value.mod}}">
		{% if value.icon %}
		<p class="{{prefix}}icon  {{mod}}">{% include "../svg-sprite/svg.twig" with {icon:value.icon} %}</p>
		{% endif %} 

		{% if value.label %}
		<p class="{{prefix}}label  {{mod}}">{{value.label}}</p>
		{% endif %}

		{% if value.name %}
		<{{value.tag}} {% if value.href %}href="{{value.target}}{{value.href}}"{% endif %} class="{{prefix}}name  {{mod}}">
			{{value.name}}
		</{{value.tag}}>
		{% endif %}

		{% if value.sublist %}
			{% for v in value.sublist %}
				<div class="{{prefix}}name  {{v.mod}}">
					{% if v.label %}
					<p class="{{prefix}}label  {{v.mod}}">{{v.label}}</p>
					{% endif %}
					<{{v.tag}} {% if v.href %}href="{{v.target}}{{v.href}}"{% endif %}>{{v.name}}</{{v.tag}}>
				</div>
			{% endfor %} 
		{% endif %}

	</div>
{% endfor %}